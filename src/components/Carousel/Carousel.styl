.lbt-wraper
  margin-top 1.66rem
  .swiper-container
    width 100%
    height 200px
    .swiper-wrapper
      .swiper-slide
        img
          width 100%
          height 100%
  .lbt-ul
    display flex
    flex-direction row
    justify-content space-around
    list-style none
    padding-left 0
    height .72rem
    li
      margin-left .1rem
      margin-right .1rem
      line-height .72rem
      a
        text-decoration none
        color #000
        font-size .26rem
        i
          color #F5DEB3
          font-size .26rem